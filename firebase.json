
{
  "firestore": {
    "rules": "firestore.rules",
    "database": "(default)"
  },
  "storage": {
    "rules": "src/storage.rules",
    "bucket": "distributors-connect-vrglt.firebasestorage.app"
  },
  "apphosting": {
    "backendId": "business-web",
