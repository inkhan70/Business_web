
{
  "firestore": {
    "rules": "firestore.rules",
    "database": "(default)"
  },
  "storage": {
    "rules": "src/storage.rules",
    "bucket": "distributors-connect-vrglt.firebasestorage.app"
  },
  "apphosting": {
    "backends": [
      {
        "id": "business-web",
        "rootDir": "/"
      }
    ],
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "functions"
    ],
    "alwaysDeployFromSource": true
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
