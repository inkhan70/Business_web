
{
  "firestore": {
    "rules": "firestore.rules",
    "database": "(default)"
  },
  "storage": {
    "rules": "src/storage.rules",
    "bucket": "distributors-connect-vrglt.firebasestorage.app"
  },
  "apphosting": {
    "backends": [
      {
        "id": "business-web",
        "repo": "inkhan70/Business_web",
        "branch": "master",
        "rootDir": "/",
        "webApp": "business-web"
      }
    ],
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "functions"
    ],
    "alwaysDeployFromSource": true
  }
}
